package reCapDemo2;

public class Main {

	public static void main(String[] args) {
		double[] myList= {1.2, 1.3, 4.3, 5.6}; //double[] myList= new double[4]; ayn覺 c覺kt覺y覺 verir.
		
		double total=0;
		for(double number : myList) {
			total=total+number;
			System.out.println(number);
		}
		
		System.out.println("Toplam = " +total);
		
	}

}
