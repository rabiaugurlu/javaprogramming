package classesWithAttributrs;

public class Product {

    private int id;
    private String name;
    private String description;
    private double price;
    private int stockAmount;
    private String renk;
    private String kod;
	
    
    public int getId() {
    	return id;
    }
    
    public void setId(int id) {
    	this.id=id;
    }
}
